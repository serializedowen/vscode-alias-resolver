{
  "name": "alias-resolver",
  "displayName": "alias-resolver",
  "description": "resolve import/require alias",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serializedowen/vscode-alias-resolver.git"
  },
  "version": "0.0.1",
  "publisher": "owen-wang",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:vue",
    "onLanguage:javascript",
    "onLanguage:javascriptreact"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.toggle",
        "title": "Toggle on/off alias resolver"
      }
    ]
  },
  "scripts": {
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.38.0",
    "eslint": "^5.13.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "@babel/parser": "^7.6.2",
    "@babel/traverse": "^7.6.2",
    "@babel/types": "^7.6.1"
  }
}
